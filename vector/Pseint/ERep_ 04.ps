Algoritmo __
	Definir v1,v2, n,m  Como Real
	Escribir "Ingresa la dimension de v1"
	leer n
	Escribir "Ingresa la dimension de v2"
	leer m
	Dimension v1(n)
	dimension v2(m)
	Dimension v3(m+n)
	Escribir "Ingresa todos los datos array 1 : ____________"
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir "Ingresa el dato: (",i, ") = " Sin Saltar
		leer v1(i)
	FinPara
	Escribir "Ingresa todos los datos array 2 : ____________"
	para i <- 1 hasta m Con Paso 1 Hacer
		Escribir "Ingresa el dato: (",i, ") = " Sin Saltar
		leer v2(i)
	FinPara
	
	j =1
	para i <- 1 hasta (n+m) Con Paso 1 Hacer
		si i <= n Entonces
			v3(i) = v1(i)
		sino 
			v3(i) = v2(j)
			j = j + 1
		FinSi
	FinPara
	
	Escribir "___________________________________________-"
	Escribir "Primer: " sin saltar
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir v1(i), ", " Sin Saltar
	FinPara
	
	Escribir " "
	Escribir "Segundo: " sin saltar
	para i <- 1 hasta m Con Paso 1 Hacer
		Escribir v2(i), ", " Sin Saltar
	FinPara
	
	Escribir " "
	Escribir "Resul: " sin saltar
	para i <- 1 hasta (m+n) Con Paso 1 Hacer
		Escribir v3(i), ", " Sin Saltar
	FinPara
	
FinAlgoritmo
