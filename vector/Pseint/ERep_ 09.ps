Algoritmo sin_titulo
	//Quishpe Patricio 
	definir v1,v2, aux Como cadena
	definir nota, nota2,aux2 Como real
	
	Escribir "Ingrese el numero de estudiantes a ingresar: " Sin Saltar
	leer n
	
	Dimension v1(n)
	Dimension v2(n)
	dimension nota(n)
	dimension nota2(n)
	
	Escribir "Ingresa todos los datos array 1 : ____________"
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir "Ingresa el nombre del estudiante : (",i, ") = " Sin Saltar
		leer v1(i)
		Escribir "Ingresa la nota del alumno: (",i, ") = " Sin Saltar
		leer nota(i)
	FinPara
	Escribir "_____________ " 
	
	//Primer listado
	Escribir "****************************"
	Escribir "1_ Listado de datos ingresados"
	Escribir "****************************"
	Escribir "No. Nombre            Nota "
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir i, "  ", v1(i), "                ", nota(i)
	FinPara
	para i <- 1 hasta n Con Paso 1 Hacer
		v2(i)= v1(i)
	FinPara
	para i <- 1 hasta n Con Paso 1 Hacer
		nota2(i)= nota(i)
	FinPara	
	//orden alfabetico
	para i <- 1 hasta n con paso 1 Hacer
		para j <- 1 hasta (n-1) con paso 1 Hacer
			si v2(j) > v2(j+1)  Entonces
				aux= v2(j)
				v2(j) = v2(j+1)
				v2(j+1) = aux
			FinSi
		FinPara
	FinPara
	
	aux2 =0
	//orden por nota
	para i <- 1 hasta n con paso 1 Hacer
		para j <- 1 hasta (n-1) con paso 1 Hacer
			si nota2(j) < nota2(j+1)  Entonces
				aux2= nota2(j)
				nota2(j) = nota2(j+1)
				nota2(j+1) = aux2
			FinSi
		FinPara
	FinPara
	//segundo  listado
	
	Escribir "****************************"
	Escribir "2_ Lista por nota"
	Escribir "****************************"
	Escribir "No. Nombre            Nota "
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir i, "  " Sin Saltar
		para j = 1 hasta n con paso 1 hacer
			si nota2(i) == nota(j) entonces
				Escribir , v1(j)	, "                ", nota(j)
			FinSi
		FinPara
	FinPara
	//Tercer listado
	
	Escribir "****************************"
	Escribir "3_ Lista por orden alfabetico"
	Escribir "****************************"
	Escribir "No. Nombre            Nota "
	para i <- 1 hasta n Con Paso 1 Hacer
		Escribir i, "  ", v2(i) Sin Saltar
		para j = 1 hasta n con paso 1 hacer
			si v2(i) == v1(j) entonces
				Escribir	, "                ", nota(j)
			FinSi
		FinPara
	FinPara
	
FinAlgoritmo
