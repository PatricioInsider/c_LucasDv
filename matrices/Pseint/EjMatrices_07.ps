Algoritmo sin_titulo
	
	definir n,v1,m1,opc Como Entero
	
	Repetir
		Escribir "Escribe un numero para generar un vector, recuerda que este se multiplicara por si mismo"
		Escribir "(no puede ser 1): "
		leer n
	Hasta Que n <> 1
	dimension v1[n*n]
	Dimension m1[n,n]
	m =n*n
	Escribir "Ingresa todos los datos array 1 : ____________"
	para i <- 1 hasta m Con Paso 1 Hacer
		Escribir "Ingresa el dato: (",i, ") = " Sin Saltar
		leer v1[i]
	FinPara
	Escribir "_____________ "
	Escribir "Tu vector ahora es una matriz::"
	Escribir "Organizar: 1)Columnas 2)Filas"
	leer opc
	Segun opc Hacer
		1:
			Escribir "Orden por columnas"
			l=1
			para i = 1 hasta n Con Paso 1 Hacer
				para j= 1 hasta n con paso 1 Hacer
					m1[j,i] = v1(l)
					l = l+1
				FinPara
			FinPara
			
		2:
			Escribir "Orden por filas"
			l=1
			para i = 1 hasta n Con Paso 1 Hacer
				para j= 1 hasta n con paso 1 Hacer
					m1[i,j] = v1(l)
					l = l+1
				FinPara
			FinPara
		
		De Otro Modo:
			Escribir "No se ha encontrado un metodo de organizabion aceptable"
	Fin Segun
	
	
	Escribir "Matriz resultado"
	para i = 1 hasta n Con Paso 1 Hacer
		para j= 1 hasta n con paso 1 Hacer
			Escribir m1[i,j], ", " Sin Saltar
		FinPara
		Escribir " "
	FinPara
	
	
FinAlgoritmo
